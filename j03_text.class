import java.util.Arrays;
import java.util.HashMap;
import java.util.Map;

public class All_Swap {
    public static void main(String[] args) {

        String [] word={"ax", "bx", "ay", "by", "ai", "aj", "bx", "by"};
        System.out.println(Arrays.toString(allSwap(word)));
    }

    private static String[] allSwap(String[] strings) {
        Map<String,Integer> map=new HashMap<>();

        for(int i=0;i< strings.length;i++){
            if(!map.containsKey(strings[i].charAt(0)+"")){ //assign first character string elements to key of map
                                                               //check it  is contains .

            map.put(strings[i].charAt(0)+"",i);   //not contains than record it.
        }else{
                int index=map.get(strings[i].charAt(0)+""); //if contains get exist key value(integer index)
                String s=strings[i];       //string [i] transferred string s
                strings[i]=strings[index]; //ax--->ay
                strings[index]=s;// ay--->ax
                map.remove(strings[i].charAt(0)+"");//to not effect coming same item removed
            }


        }
            return strings;
    }
}
